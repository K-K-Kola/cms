{
  "version": 2,
  "builds": [
    {
      "src": "apps/cms-api/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["../../dist/apps/cms-api"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/cms-api/main.ts"
    }
  ],
  "devCommand": "cd ../../ && npx nx run cms-api:serve --configuration=production",
  "buildCommand": "cd ../../ && npx nx run cms-api:build",
  "outputDirectory": "../../dist/apps/cms-api",
  "installCommand": "npm install"

}
